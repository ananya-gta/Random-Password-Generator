        "s=\"abcdefghijklmnopqrstuvwxyz01234567890ABCDEFGHIJKLMNOPQRSTUVWXYZ!@#$%^&*()?\"\n",
        "pw= \"\".join(random.sample(s,pw_length))\n",
        "print(pw)"
